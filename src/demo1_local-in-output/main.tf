terraform {
  required_providers {
    random = {
      source  = "hashicorp/random"
      version = "~> 3.0"
    }
  }
}

##################################################################################
# PROVIDERS
##################################################################################

provider "random" {
}

##################################################################################
# RESOURCES
##################################################################################

resource "random_integer" "random_int" {
  min = 1
  max = 100
}